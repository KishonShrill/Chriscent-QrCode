{
  "name": "qr-code-component",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "publicdev": "vite --host",
    "start": "concurrently \"vite --host\" \"node public/server.cjs\"",
